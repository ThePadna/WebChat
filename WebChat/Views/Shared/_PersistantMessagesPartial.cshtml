@using System.Diagnostics;
<script>
    console.log("test");

</script>
@{
    if (PersistantMessagesPartialModel.Messages == null)
    {
        Debug.WriteLine("Null");
    }
    else {
        Debug.WriteLine("NotNull");
        foreach(var m in PersistantMessagesPartialModel.Messages)
        {
            <script>
                window.onload = function () {
                    $('#messages').append("[@m.Date] " + "@m.Sender" + ": " + "@m.Contents");
                };
            </script>
        }
    }
}
